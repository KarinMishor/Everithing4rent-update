<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="384.0" prefWidth="537.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GUIController">
    <children>
        <ImageView fx:id="iv1" fitHeight="261.0" fitWidth="361.0" layoutX="76.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@logo.jpg" />
            </image></ImageView>
        <Button fx:id="loginApp" layoutX="86.0" layoutY="244.0" mnemonicParsing="false" onAction="#login" prefHeight="32.0" prefWidth="102.0" text="התחבר/הרשם">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button fx:id="addProducts" disable="true" layoutX="199.0" layoutY="245.0" mnemonicParsing="false" onAction="#setMyProducts" prefHeight="32.0" prefWidth="92.0" text="הוספת חבילה">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button fx:id="myOrders" disable="true" layoutX="200.0" layoutY="293.0" mnemonicParsing="false" onAction="#myOrdes" prefHeight="32.0" prefWidth="92.0" text="הזמנות שלי">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button fx:id="logOut_btn" disable="true" layoutX="85.0" layoutY="292.0" mnemonicParsing="false" onAction="#logOut" prefHeight="32.0" prefWidth="102.0" text="התנתק">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button fx:id="myProducts" disable="true" layoutX="301.0" layoutY="245.0" mnemonicParsing="false" onAction="#MyProducts" prefHeight="32.0" prefWidth="92.0" text="הפריטים שלי">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>

        <Button fx:id="searchProduct" disable="false" layoutX="403.0" layoutY="294.0" mnemonicParsing="false" onAction="#searchProduct" prefHeight="32.0" prefWidth="92.0" text="חיפוש מוצר">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
      <Button fx:id="newOrder_btn" disable="true" layoutX="301.0" layoutY="293.0" mnemonicParsing="false" onAction="#createNewOrder" prefHeight="32.0" prefWidth="92.0" text="הזמנת חבילה">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>

    </children>
</AnchorPane>
